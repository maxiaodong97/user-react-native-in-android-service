<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.reactnativeandroidservice">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
      android:allowBackup="true"
      android:label="@string/app_name"
      android:icon="@mipmap/ic_launcher"
      android:name="com.reactnativeandroidservice.App"
      android:theme="@style/AppTheme">
      <activity
        android:name=".MainActivity"
        android:label="@string/app_name"
        android:configChanges="keyboard|keyboardHidden|orientation|screenSize">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />
            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
      </activity>
      <activity android:name="com.facebook.react.devsupport.DevSettingsActivity" />
        <service android:name=".ExampleIntentService" android:exported="false">
            <intent-filter>
                <action android:name="com.reactnativeandroidservice.ExampleIntentService" />
            </intent-filter>
        </service>
    </application>

</manifest>
